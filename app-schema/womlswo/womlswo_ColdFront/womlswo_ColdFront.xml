<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
    <namespaces>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml/3.2</uri>
        </Namespace>
        <Namespace>
            <prefix>womlswo</prefix>
            <uri>http://xml.fmi.fi/namespace/woml/swo/2011/11/15</uri>
        </Namespace>
        <Namespace>
            <prefix>womlcore</prefix>
            <uri>http://xml.fmi.fi/namespace/woml/core/2011/11/15</uri>
        </Namespace>
        <Namespace>
            <prefix>xlink</prefix>
            <uri>http://www.w3.org/1999/xlink</uri>
        </Namespace>
    </namespaces>
    <includedTypes>
        <Include>../../womlcore/common-core-nonFeature/TimeInterpolation.xml</Include>
        <Include>../../womlcore/common-core-nonFeature/elevation.xml</Include>
 <!--       <Include>../../womlcore/common-core-nonFeature/uncertaintyArea.xml</Include>-->
    </includedTypes>
    <sourceDataStores>
        <DataStore>
          <id>datastore</id>
                                        <parameters>
                                            <Parameter>
                                                <name>dbtype</name>
                                                <value>postgis</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>host</name>
                                                <value>localhost</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>port</name>
                                                <value>5432</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>database</name>
                                                <value>woml</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>user</name>
                                                <value>postgres</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>passwd</name>
                                                <value>asd123zzz</value>
                                            </Parameter>
                                        </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>http://xml.fmi.fi/schema/woml/swo/2011/11/15/woml-swo.xsd</schemaUri>
            <schemaUri>http://xml.fmi.fi/schema/woml/core/2011/11/15/woml-core.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>cold_front</sourceType>
            <targetElement>womlswo:ColdFront</targetElement>
            <attributeMappings>
            
                <AttributeMapping>
                    <targetAttribute>
                        womlswo:ColdFront
                    </targetAttribute>
                    <idExpression>
                        <OCQL>gml_id</OCQL>
                    </idExpression>
                </AttributeMapping>

                <AttributeMapping>
                    <targetAttribute>
                        FEATURE_LINK[1]
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>associated</OCQL>
                    </sourceExpression>
                </AttributeMapping>

                <AttributeMapping>
                    <targetAttribute>gml:validTime</targetAttribute>
                    <targetAttributeNode>gml:TimePeriodPropertyType</targetAttributeNode>
                </AttributeMapping>
                
                <AttributeMapping>
                    <targetAttribute>gml:validTime/gml:TimePeriod</targetAttribute>
                    <ClientProperty>
                        <name>gml:id</name>
                        <value>time_period_gml_id</value>
                    </ClientProperty>
                </AttributeMapping>

                <AttributeMapping>
                    <targetAttribute>gml:validTime/gml:TimePeriod/gml:beginPosition</targetAttribute>
                        <sourceExpression>
                            <OCQL>time_period_begin_position</OCQL>
                        </sourceExpression>
                </AttributeMapping>

                <AttributeMapping>
                    <targetAttribute>gml:validTime/gml:TimePeriod/gml:endPosition</targetAttribute>
                        <sourceExpression>
                            <OCQL>time_period_end_position</OCQL>
                        </sourceExpression>
                </AttributeMapping>               
