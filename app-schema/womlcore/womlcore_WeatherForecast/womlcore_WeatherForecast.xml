                    <targetAttribute>
                        womlcore:latestModificationTime
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>latest_modification_time</OCQL>
                    </sourceExpression>
                </AttributeMapping>


                <AttributeMapping>
                    <targetAttribute>
                        womlcore:shortInfo
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>short_info</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>xml:lang</name>
                        <value>'Ita-it'</value>
                    </ClientProperty>
                </AttributeMapping>
                
                <AttributeMapping>
                    <targetAttribute>
                        womlcore:longInfo
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>long_info</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>xml:lang</name>
                        <value>'Ita-it'</value>
                    </ClientProperty>
                </AttributeMapping>

