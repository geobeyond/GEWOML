                    <sourceExpression>
                        <OCQL>id</OCQL>
                        <linkElement>womlswo:ColdFront</linkElement>
                        <linkField>FEATURE_LINK[1]</linkField>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                </AttributeMapping>

