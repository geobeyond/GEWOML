<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
    <namespaces>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml/3.2</uri>
        </Namespace>
        <Namespace>
            <prefix>womlcore</prefix>
            <uri>http://xml.fmi.fi/namespace/woml/core/2011/11/15</uri>
        </Namespace>
    </namespaces>
   <sourceDataStores>
        <DataStore>
          <id>datastore</id>
                                        <parameters>
                                            <Parameter>
                                                <name>dbtype</name>
                                                <value>postgis</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>host</name>
                                                <value>localhost</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>port</name>
                                                <value>5432</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>database</name>
                                                <value>woml</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>user</name>
                                                <value>postgres</value>
                                            </Parameter>
                                            <Parameter>
                                                <name>passwd</name>
                                                <value>asd123zzz</value>
                                            </Parameter>
                                        </parameters>
        </DataStore>
    </sourceDataStores>
    <catalog>../../../schemas/catalog.xml</catalog>
    <targetTypes>
        <FeatureType>
            <schemaUri>http://xml.fmi.fi/schema/woml/core/2011/11/15/woml-core.xsd</schemaUri>
        </FeatureType>

    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>geographic_region</sourceType>
            <targetElement>womlcore:GeographicRegion</targetElement>

            <attributeMappings>

                <AttributeMapping>
                    <targetAttribute>
                        womlcore:GeographicRegion
                    </targetAttribute>
                    <idExpression>
                        <OCQL>getId()</OCQL>
                    </idExpression>
                </AttributeMapping>
                
                
                
                <AttributeMapping>
                    <targetAttribute>
                        womlcore:regionId
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>strTrim(region_id)</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>scheme</name>
                        <value>authority</value>
                    </ClientProperty>
                </AttributeMapping>
                
                <AttributeMapping>
                    <targetAttribute>
                        womlcore:localizedName
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>strTrim(localized_name)</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>xml:lang</name>
                        <value>'IT-it'</value>
                    </ClientProperty>
                </AttributeMapping>









            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</as:AppSchemaDataAccess>
